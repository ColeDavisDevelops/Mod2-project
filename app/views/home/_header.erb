<%= link_to "Home", "/" %>--
<% Subreddit.all.each do |s|%>
<%= link_to s.name, s %>- 
<% end %>--
<%= link_to 'Stats', "/scores" %>--

<% if session[:user_id] %>
    <%= link_to "Profile", "/users/#{session[:user_id]}"  %>
    <%= form_with method: 'delete', url: "/logout" do |f| %>
    <%= f.submit "logout" %>
    <% end %>
<% else %>
    <%= link_to "Log In/New User", "/login"  %>
<% end %>--
<hr>