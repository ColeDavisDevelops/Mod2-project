<p>create new user</p>

<%= form_with model: @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :password %>
<%= f.text_field :password %>
<br>
<%= f.label :bio %>
<%= f.text_field :bio %>
<br>
<%= f.label :profile_img_url %>
<%= f.text_field :profile_img_url %>

<%= f.submit %>

<% end %>

